import java.util.*;

class Main {
    public static void main(String[] args) {
        Scanner io = new Scanner(System.in);
        String name = io.next();
        Set<Character> s = new LinkedHashSet<>(); 
        for (char ch : name.toCharArray()) {
            s.add(ch);
        }
        
        StringBuilder names = new StringBuilder();
        for (char ch : s) {
            names.append(ch);
        }
        
        System.out.println(names);
    }
}
